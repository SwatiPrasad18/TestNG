<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="10">
	<test thread-count="5" name="Simple">
		<classes>
			<class name="annotations.Sequence" />
		</classes>
	</test> <!-- Test -->

	<test name="Regression">
	<groups>
		<run>
			<include name="smoke"></include>
		</run>
	</groups>
		<classes>
			<class name="annotations.Groups" />
		</classes>
	</test>
	
	<test name ="Functional">
	<groups>
		<run>
			<include name="functional"></include>
		</run>
	</groups>
	
	<classes>
		<class name="annotations.Groups"></class>
	</classes>
	</test>
</suite> <!-- Suite -->
